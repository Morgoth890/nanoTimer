<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="BorderTableLayout">
    <attr name="borderColor" format="color"/>
    <attr name="borderSize" format="integer"/>
    <attr name="scaleBorders" format="boolean"/>
  </declare-styleable>
  <declare-styleable name="FontFitTextView">
    <attr name="myfont">
      <enum name="MONOSPACE" value="0"/>
    </attr>
  </declare-styleable>
  <declare-styleable name="NumberLimit">
    <attr name="min" format="integer"/>
    <attr name="max" format="integer"/>
    <attr name="defaultVal" format="integer"/>
    <attr name="cyclic" format="boolean"/>
  </declare-styleable>
</resources>